<h1>会員一覧</h1>
<div class = "users_index_table">
  <table class = "table table-bordered">
    <thead>
      <tr>
       <th>会員ID:</th>
       <th>名前:</th>
       <th>メールアドレス:</th>
       <th>ステータス:</th>
       <th>詳細情報：</th>
      </tr>
    </thead>
    <tbody>
      <tr>
       <% @users.each do |user| %>
        <td><%= user.id %></td>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%#= user.status %></td>
        <td><%= link_to "詳細を見る",admin_users_path(user), class: 'btn btn-success' %></td>
       <% end %>
      </tr>
    </tbody>
  </table>

</div>
<%#= paginete @users %>